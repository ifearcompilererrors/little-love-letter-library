{
  "name": "little-love-letters-library",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3001/",
  "main": "index.js",
  "scripts": {
    "build": "react-scripts build",
    "start:server": "nodemon index.js --watch index.js server/*",
    "start:client": "BROWSER=none react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\" --kill-others",
    "local": "ENVIRONMENT=development npm run dev",
    "start": "npm run dev"
  },
  "type": "module",
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "babel-core": "^6.26.3",
    "express": "^4.21.2",
    "googleapis": "^144.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
